{
    "version": "1.0",
    "type": "form",
    "rootContainers": [
        {
            "label": "Main",
            "container": "main"
        }
    ],
    "containers": [
        {
            "id": "main",
            "rows": [
                [
                    {
                        "model": "name",
                        "container": "name"
                    }
                ],
                [
                    {
                        "model": "addresses",
                        "item": {
                            "container": "addr",
                            "inline": true,
                            "label": "Address"
                        }
                    }
                ]
            ]
        },
        {
            "id": "name",
            "rows": [
                [
                    {
                        "model": "first",
                        "className": "col-md-6"
                    },
                    {
                        "model": "last",
                        "className": "col-md-6"
                    }
                ]
            ]
        },
        {
            "id": "addr",
            "defaultClassName": "col-md-4",
            "rows": [
                [
                    {
                        "model": "street",
                        "className": "col-md-12"
                    }
                ],
                [
                    {
                        "model": "city"
                    },
                    {
                        "model": "state"
                    },
                    {
                        "model": "zip"
                    }
                ]
            ]
        }
    ]
}
